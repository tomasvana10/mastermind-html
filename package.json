{
  "name": "mastermind-html",
  "version": "1.2.0",
  "description": "The code-breaking board game in HTML ",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "npx eslint .",
    "prettier": "npx prettier . --write",
    "prepare": "husky && husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,html,mjs}": [
      "npx prettier . --write",
      "npx eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasvana10/mastermind-html.git"
  },
  "keywords": [
    "puzzle",
    "mastermind",
    "boardgame",
    "html",
    "js",
    "css"
  ],
  "author": "Tomas Vana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomasvana10/mastermind-html/issues"
  },
  "homepage": "https://github.com/tomasvana10/mastermind-html#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.14",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "chalk": "^4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "lint-staged": "^15.4.1",
    "mkdirp": "^3.0.1",
    "prettier": "^3.4.2",
    "puppeteer": "^24.1.0",
    "rimraf": "^6.0.1"
  }
}
